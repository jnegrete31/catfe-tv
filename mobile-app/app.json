{
  "expo": {
    "name": "Catf√© TV",
    "slug": "catfe-tv",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF6E3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.catfe.tv",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      }
    },
    "tvos": {
      "icon": "./assets/tv-icon.png",
      "bundleIdentifier": "com.catfe.tv.tvos",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FDF6E3"
      },
      "package": "com.catfe.tv"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "catfe-tv"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "catfe-tv"
  }
}
